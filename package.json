{
  "name": "sanity-plugin-markdown",
  "version": "3.0.0-v3-studio.0",
  "description": "Markdown fields in Sanity Studio. Supports Github flavored Markdown and image uploads.",
  "scripts": {
    "clean": "rimraf lib",
    "prebuild": "npm run clean && plugin-kit verify-package --silent",
    "build": "parcel build --no-cache",
    "watch": "parcel watch",
    "link-watch": "plugin-kit link-watch",
    "lint": "eslint .",
    "format": "prettier src -w",
    "prepare": "husky install",
    "compile": "tsc --noEmit",
    "prepublishOnly": " npm run build"
  },
  "source": "./src/index.ts",
  "main": "./lib/index.js",
  "module": "./lib/index.modern.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "default": "./lib/index.modern.js"
    }
  },
  "files": [
    "src",
    "lib"
  ],
  "keywords": [
    "sanity",
    "sanity-plugin",
    "studio",
    "markdown",
    "github flavored markdown"
  ],
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "dependencies": {
    "@reach/auto-id": "^0.17.0",
    "@sanity/incompatible-plugin": "^1.0.0",
    "@sanity/ui": "^0.37.9",
    "@uiw/react-md-editor": "^3.18.1",
    "rehype-sanitize": "^5.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@parcel/packager-ts": "^2.7.0",
    "@parcel/transformer-typescript-types": "^2.7.0",
    "@sanity/plugin-kit": "^1.0.1",
    "@sanity/semantic-release-preset": "^2.0.0",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-sanity": "^5.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "husky": "^7.0.0",
    "lint-staged": "^12.3.3",
    "parcel": "^2.7.0",
    "prettier": "^2.5.1",
    "react": "^17.0.0 || ^18.0.0",
    "rimraf": "^3.0.2",
    "sanity": "3.0.0-dev-preview.15",
    "styled-components": "^5.2.0",
    "typescript": "^4.8.2"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "sanity": "dev-preview",
    "styled-components": "^5.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/sanity-plugin-markdown.git"
  },
  "bugs": {
    "url": "https://github.com/sanity-io/sanity-plugin-markdown/issues"
  },
  "homepage": "https://sanity.io",
  "sanityExchangeUrl": "https://www.sanity.io/plugins/sanity-plugin-markdown",
  "engines": {
    "node": ">=14.0.0"
  }
}
